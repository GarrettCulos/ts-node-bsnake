{
  "name": "yeti.snake",
  "version": "0.0.1",
  "description": "TypeScript Nodejs Battle Snake, I dube the, the Yeti.",
  "repository": {
    "type": "git",
    "url": "https://github.com:GarrettCulos/ts-node-bsnake.git"
  },
  "author": "Garrett Culos",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "cr": "npm run build && npm run test",
    "prettier:check": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:fix": "prettier --config ./.prettierrc --staged --write \"src/**/*\"",
    "prettier:quick": "pretty-quick --stage"
  },
  "precommit": [
    "prettier:quick"
  ],
  "dependencies": {
    "@types/async": "^2.0.50",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/glob": "^5.0.36",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.118",
    "@types/lusca": "^1.5.0",
    "@types/mime": "^2.0.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.39",
    "@types/request": "^2.48.1",
    "@types/serve-static": "^1.13.2",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.6",
    "@types/winston": "^2.4.4",
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "compression": "^1.7.3",
    "concurrently": "^3.6.1",
    "cron": "^1.5.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "google-auth-library": "^2.0.1",
    "jest": "^22.4.4",
    "lodash": "^4.17.11",
    "lusca": "^1.6.1",
    "mime": "^2.3.1",
    "minimatch": "^3.0.4",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "precommit": "^1.2.2",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "winston": "^2.4.4"
  }
}
